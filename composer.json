{
  "name": "ark4ne/laravel-openapi-generator",
  "description": "OpenApi - Laravel Documentation Generator",
  "keywords": [
    "laravel",
    "openapi",
    "documentation",
    "generator"
  ],
  "type": "library",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0|^9.0",
    "goldspecdigital/oooas": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "1.6.x-dev",
    "orchestra/testbench": "^7.5"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ark4ne\\OpenApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ark4ne\\OpenApi\\Providers\\LaravelOpenApiProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
